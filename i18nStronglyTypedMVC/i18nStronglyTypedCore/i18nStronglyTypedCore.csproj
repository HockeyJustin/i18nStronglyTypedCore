<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>*See "source code" link for tutorial* (github). This is a dotnet core port of afana.me's MVC5 internationalization i18n. Converted to a nuget package to save typing and with additional methods for flexibility. Unlike dotnetcore's inbuilt localisation, this is strongly typed (as much as possible)</Description>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/HockeyJustin/i18nStronglyTypedCore</RepositoryUrl>
    <PackageTags>i18n mvc internationalisation csharp dotnetcore dotnet core</PackageTags>
    <AssemblyVersion>1.0.0.3</AssemblyVersion>
    <FileVersion>1.0.0.3</FileVersion>
    <Version>1.0.0.3</Version>
    <PackageReleaseNotes>1.0.0.3 - Can now have multiple resource files</PackageReleaseNotes>
  </PropertyGroup>

</Project>
